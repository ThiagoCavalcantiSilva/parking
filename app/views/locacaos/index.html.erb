<p id="notice"><%= notice %></p>

<h1>Locacaos</h1>

<table>
  <thead>
    <tr>
      <th>Placa</th>
      <th>Vaga</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locacaos.each do |locacao| %>
      <tr>
        <td><%= locacao.placa %></td>
        <td><%= locacao.vaga %></td>
        <td><%= link_to 'Show', locacao %></td>
        <td><%= link_to 'Edit', edit_locacao_path(locacao) %></td>
        <td><%= link_to 'Destroy', locacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Locacao', new_locacao_path %>
